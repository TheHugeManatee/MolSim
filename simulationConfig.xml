<?xml version="1.0"?>

<simulationConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="simulationConfig.xsd">
       
  <deltaT>0.0003</deltaT>
  <endTime>6</endTime>
  <scenarioType>Lennard-Jones</scenarioType>
  <domainSize x0="30.0" x1="30.0" x2="30.0" />
  <cutoffRadius>3.0</cutoffRadius>

  <boundaryHandling
  	top="Periodic" bottom="Periodic"
  	left="Periodic" right="Periodic"
  	front="Periodic" back="Periodic"
  />
  <containerType>CelllistContainer</containerType>
  <epsilon>5</epsilon>
  <sigma>1</sigma>
  <gravitationConstant>9.89</gravitationConstant>
  <outputFrequency>100</outputFrequency>
  <disableOutput>0</disableOutput>
  <loggerConfigFile>Logger.properties</loggerConfigFile>
  <outputFilePrefix>OutputFiles/MD_vtk</outputFilePrefix>
  <outputFileType>vtk</outputFileType>
  <inputFile></inputFile>
  
  <thermostatSwitch>ON</thermostatSwitch>

  <thermostatSettings>
  	<initTemperature>40.0</initTemperature>
  	<targetTemperature>100.0</targetTemperature>
  	<controlInterval>100</controlInterval>
  	<estimatedSteps>20000</estimatedSteps>
  	<maxSteps>30000</maxSteps>
  </thermostatSettings>

 <generator>
   <cuboid>
      <bottomLeft x0="10.0" x1="15.0" x2="10.0" />
      <nX x0="5" x1="5" x2="5" />
      <stepWidth>1.1225</stepWidth>
      <mass>1</mass>
      <type>1</type>
      <initialVelocity x0="0" x1="0" x2="0"/>
      <brownianMeanVelocity>0</brownianMeanVelocity>
    </cuboid>
    <cuboid>
      <bottomLeft x0="10.0" x1="5.0" x2="10.0" />
      <nX x0="5" x1="5" x2="5" />
      <stepWidth>1.1225</stepWidth>
      <mass>1</mass>
      <type>1</type>
      <initialVelocity x0="0" x1="0" x2="0"/>
      <brownianMeanVelocity>0</brownianMeanVelocity>
    </cuboid>
    
  </generator>
  

</simulationConfig>
